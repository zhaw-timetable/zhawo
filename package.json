  "bugs": {
    "url": "https://github.com/zhaw-timetable/zhawo/issues"
  },
  "homepage": "https://github.com/zhaw-timetable/zhawo#readme",
  "dependencies": {
    "concurrently": "^4.0.1"
  },
  "devDependencies": {
    "husky": "^1.1.2"
  }
}
