{
  "name": "zhaw-timetable-frontend",
  "version": "0.0.1",
  "description": "A timetable progressive web application for ZHAW School of Engineering",
  "main": "main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --history-api-fallback --mode=development --hot",
    "build": "cross-env NODE_ENV=production webpack --mode=production ",
    "test": "jest --coverage",
    "test-only": "jest",
    "watch": "jest --watch",
    "codecov": "codecov",
    "clean": "rm -rf node_modules && rm -rf bundle",
    "reinstall": "npm run clean && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhaw-timetable/zhawo.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zhaw-timetable/zhawo/issues"
  },
  "homepage": "https://github.com/zhaw-timetable/zhawo#readme",
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "autosuggest-highlight": "^3.1.1",
    "cross-env": "^5.2.0",
    "date-fns": "^1.30.1",
    "flux": "^3.1.3",
    "idb": "^2.1.3",
    "react": "^16.8.6",
    "react-autosuggest": "^9.4.3",
    "react-dom": "^16.8.6",
    "react-easy-swipe": "0.0.17",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^24.0.13",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "codecov": "^3.3.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "jest-enzyme": "^7.0.2",
    "jest-fetch-mock": "^1.7.5",
    "node-sass": "^4.13.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-manifest-plugin": "^2.0.4",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "jest": {
    "automock": false,
    "setupFilesAfterEnv": [
      "./app/setupJest.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss|sass)$": "babel-jest"
    },
    "unmockedModulePathPatterns": [
      "react",
      "enzyme",
      "jest-enzyme"
    ]
  }
}
